        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-wrapper": {
              "overflow": "hidden"
            },
            ".card-wrapper > *:not(.mbr-overlay), .card-wrapper": {
              "position": "relative"
            },
            ".card-wrapper when not (@transparentBg)": {
              ".mbr-overlay": {
                "background-color": "@cardBg"
              },
              ".card-head": {
                "background-color": "fade(@secondaryColor, 12%)"
              }
            },
            ".card-wrapper when (@showBorder)": {
              "border": "1px solid @colorBorder"
            },
            ".amp-iconfont when (@showIcons)": {
              "font-size": "~\"@{iconsSize}rem\"",
              "width": "~\"@{iconsSize}rem\"",
              "color": "@iconsColor",
              "& when ((@iconsSize * 16) > 40)": {
                "@media (max-width: 767px)": {
                  "font-size": "40px",
                  "width": "40px"
                }
              }
            },
            ".card-price-wrap": {
              "align-items": "center",
              ".card-price": {
                "align-items": "flex-start"
              }
            },
            ".card when not (@transparentBg), (@showBorder)": {
              ".card-head": {
                "padding": "45px 35px 30px",
                "@media (max-width: 991px)": {
                  "padding": "25px 20px 20px"
                }
              },
              ".list-item": {
                "border-bottom": "1px solid",
                "& when not (@transparentBg)": {
                  "border-color": "darken(@cardBg, 9.2%)"
                },
                "& when (@showBorder)": {
                  "border-color": "@colorBorder"
                }
              },
              "&.md-pb when (@cardsAmount = 4)": {
                "@media (max-width: 991px)": {
                  "padding-bottom": "20px"
                }
              },
              "@media (max-width: 767px)": {
                "&:not(.last-child)": {
                  "padding-bottom": "20px"
                }
              },
              "& when (@showFavorite)": {
                "&.favoritePrice": {
                  ".card-wrapper:before": {
                    "content": "'BEST'",
                    "position": "absolute",
                    "right": "-25px",
                    "top": "10px",
                    "font-size": "12px",
                    "font-weight": "600",
                    "transform": "rotate(45deg)",
                    "background-color": "@primaryColor",
                    "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%)",
                    "padding": "5px 30px",
                    "line-height": "initial",
                    "z-index": "1"
                  },
                  ".card-head": {
                    "background-color": "@favoriteBgColor",
                    "> *, .amp-iconfont": {
                      "color": "@favoriteTextColor !important"
                    }
                  }
                },
                "&:not(.favoritePrice) .card-wrapper when (@showEffect) and not (@cardsAmount = '1')": {
                  ".card-head > *, .card-head, .amp-iconfont": {
                    "transition": "all 0.2s"
                  },
                  "&:hover .card-head": {
                    "background-color": "@favoriteBgColor",
                    "> *, .amp-iconfont": {
                      "color": "@favoriteTextColor !important"
                    }
                  }
                }
              }
            },
            "& when (@showMilestone) and not (@titleAlign = '2')": {
              ".milestone, .milestone > div, .milestone > div > div": {
                "width": "100%"
              },
              ".milestone > div > div": {
                "padding-left": "0",
                "padding-right": "0"
              },
              ".milestone > div > div when (@showBorderBottom)": {
                "border-bottom": "1px solid @milBorderColor"
              },
              ".milestone .mbr-text": {
                "padding-top": "5px"
              },
              ".title .milestone > div when (@showTitle), (@showSubtitle)": {
                "& when (@milCount > 2)": {
                  "&:nth-child(1), &:nth-child(2)": {
                    "padding-bottom": "20px"
                  }
                }
              },
              "@media (max-width: 767px)": {
                ".title-wrap": {
                  "padding-bottom": "20px"
                },
                ".milestone > div:not(:last-child)": {
                  "padding-bottom": "20px"
                },
                ".milestone .numeral": {
                  "justify-content": "center"
                }
              }
            },
            ".card-price-wrap when (@cardAlign = '3')": {
              "@media (max-width: 767px)": {
                "justify-content": "center"
              }
            },
            ".items-list, .card-head .mbr-text": {
              "color": "#84848c"
            }
          },
          "_name": "pricing-table1",
          "_customHTML": "<section class=\"pricing-table1\" group=\"Pricing Tables\" plugins=\"amp-main\">\n\t<mbr-parameters>\n\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\t\t<!--  -->\n\t\t<header>Title</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n            <input type=\"checkbox\" title=\"Milestone With Number\" name=\"showMilestone\" condition=\"titleAlign != 2 || (!showTitle && !showSubtitle)\">\n                <input type=\"checkbox\" title=\"Border Bottom\" name=\"showBorderBottom\" condition=\"showMilestone && (titleAlign != 2 || (!showTitle && !showSubtitle))\">\n                <input type=\"color\" title=\"Border Color\" name=\"milBorderColor\" value=\"#bec1c7\" condition=\"showMilestone && showBorderBottom && (titleAlign != 2 || (!showTitle && !showSubtitle))\" checked>\n            <input type=\"range\" inline title=\"Count Milestone\" name=\"milCount\" min=\"1\" max=\"4\" step=\"1\" value=\"2\" condition=\"showMilestone && (titleAlign != 2 || (!showTitle && !showSubtitle))\">\n            <input type=\"checkbox\" title=\"Reverse Content Title\" name=\"reverseContent\" condition=\"showMilestone && (showTitle || showSubtitle) && titleAlign != 2\">\n            <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n                <option value=\"1\">Left</option>\n                <option value=\"2\" selected>Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n\t\t<header>Cards</header>\n\t\t<input type=\"range\" inline title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Price Subtitle\" name=\"showPriceSubtitle\" checked>\n\t\t<input type=\"range\" inline title=\"Items in 1\" name=\"card1Items\" min=\"1\" max=\"6\" step=\"1\" value=\"1\">\n\t\t<input type=\"range\" inline title=\"Items in 2\" name=\"card2Items\" min=\"1\" max=\"6\" step=\"1\" value=\"1\" condition=\"cardsAmount > 1\">\n\t\t<input type=\"range\" inline title=\"Items in 3\" name=\"card3Items\" min=\"1\" max=\"6\" step=\"1\" value=\"1\" condition=\"cardsAmount > 2\">\n\t\t<input type=\"range\" inline title=\"Items in 4\" name=\"card4Items\" min=\"1\" max=\"6\" step=\"1\" value=\"1\" condition=\"cardsAmount > 3\">\n        <input type=\"checkbox\" title=\"Price Text\" name=\"showPriceText\" checked>\n\t\t<input type=\"checkbox\" title=\"Icons\" name=\"showIcons\" checked>\n\t\t    <input type=\"range\" inline title=\"Size\" name=\"iconsSize\" min=\"1\" max=\"8\" step=\"0.1\" value=\"3.9\" condition=\"showIcons\">\n            <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#194f82\" condition=\"showIcons\">\n        <select title=\"Align\" name=\"cardAlign\">\n\t        <option value=\"1\">Left</option>\n\t        <option value=\"2\" selected>Center</option>\n\t        <option value=\"3\">Right</option>\n    \t</select>\n\t\t<input type=\"checkbox\" name=\"showBorder\" title=\"Border\">\n\t\t<input type=\"color\" name=\"colorBorder\" title=\"Color\" value=\"#cccccc\" condition=\"showBorder\">\n\t\t<input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\">\n        <input type=\"color\" title=\"Card BG\" name=\"cardBg\" value=\"#f7f8f8\" condition=\"!transparentBg\">\n        <input type=\"checkbox\" title=\"Just Favorite\" name=\"showFavorite\" checked condition=\"!transparentBg || showBorder\">\n        <select title=\"Favorite Item\" name=\"favoriteItem\" condition=\"showFavorite && (!transparentBg || showBorder)\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <input type=\"color\" title=\"Favorite Bg\" name=\"favoriteBgColor\" value=\"#194f82\" condition=\"showFavorite && (!transparentBg || showBorder)\">\n        <input type=\"color\" title=\"Favorite Color Text\" name=\"favoriteTextColor\" value=\"#ffffff\" condition=\"showFavorite && (!transparentBg || showBorder)\">\n        <input type=\"checkbox\" title=\"Effect Hover\" name=\"showEffect\" condition=\"showFavorite && (!transparentBg || showBorder)\" checked>\n\t\t<!--  -->\n\t\t<header>Background</header>\n            <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n            <input type=\"range\" inline title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"twoBgColor && bg.type == 'color'\">    \n            <fieldset type=\"background\" name=\"bg\">\n                <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/b01.jpg\">\n                <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n            </fieldset>\n            <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f4f5f7\" condition=\"twoBgColor && bg.type == 'color'\">  \n            <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n            <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n            <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay && bg.type !== 'color') || (twoBgColor && bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 && twoBgColor && bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 && twoBgColor && bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 && twoBgColor && bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 && twoBgColor && bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 && twoBgColor && bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"mbr-row title mbr-pb-4\" mbr-if=\"showTitle || showSubtitle || showMilestone\" mbr-class=\"{'mbr-row-reverse':showMilestone && (showTitle || showSubtitle) && titleAlign != 2 && reverseContent, 'mbr-px-2': fullWidth, 'align-left': titleAlign == 1, 'align-center': titleAlign == 2, 'align-right': titleAlign == 3}\">\n            <div class=\"title-wrap mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-4': showMilestone && titleAlign != 2, 'mbr-col-md-12':!showMilestone || titleAlign == 2}\" mbr-if=\"showTitle || showSubtitle\">        \n                <h2 class=\"mbr-section-title mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">Featured Offers from Our Partners</h2>\n                <h3 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">Pricing Table Subtitle</h3>\n            </div>\n            <div class=\"milestone mbr-flex flex-wrap\" mbr-if=\"showMilestone && titleAlign != 2\" mbr-list mbr-list-grow=\"milCount\" mbr-class=\"{'mbr-col-sm-12 mbr-col-md-8': showTitle || showSubtitle}\">\n                <div mbr-class=\"{'mbr-col-xl-4':milCount == 3 && fullWidth, 'mbr-col-md-6 mbr-col-lg-6': showTitle || showSubtitle, 'mbr-col-sm-12 mbr-col-md-3': !showTitle && !showSubtitle}\">\n                    <div class=\"mbr-m-auto\" mbr-class=\"{'mbr-pb-4': showBorderBottom, 'mbr-col-md-8':showTitle || showSubtitle, 'mbr-col-md-12 mbr-col-lg-8':!showTitle && !showSubtitle}\">\n                        <div class=\"numeral mbr-flex\" mbr-class=\"{'mbr-jc-e': titleAlign == 3}\">\n                            <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".milestone .numeral\">\n                                100\n                            </h4>\n                            <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" data-app-selector=\".milestone .numeral\">%</h4>\n                        </div>\n                        <p class=\"mbr-text mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" data-app-selector=\".milestone .mbr-text\">\n                           Lorem ipsum\n                        </p>\n                    </div>\n                </div>\n            <div mbr-class=\"{'mbr-col-xl-4':milCount == 3 && fullWidth, 'mbr-col-md-6 mbr-col-lg-6': showTitle || showSubtitle, 'mbr-col-sm-12 mbr-col-md-3': !showTitle && !showSubtitle}\">\n                    <div class=\"mbr-m-auto\" mbr-class=\"{'mbr-pb-4': showBorderBottom, 'mbr-col-md-8':showTitle || showSubtitle, 'mbr-col-md-12 mbr-col-lg-8':!showTitle && !showSubtitle}\">\n                        <div class=\"numeral mbr-flex\" mbr-class=\"{'mbr-jc-e': titleAlign == 3}\">\n                            <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".milestone .numeral\">\n                                100\n                            </h4>\n                            <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" data-app-selector=\".milestone .numeral\">%</h4>\n                        </div>\n                        <p class=\"mbr-text mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" data-app-selector=\".milestone .mbr-text\">\n                           Lorem ipsum</p>\n                    </div>\n                </div></div>\n        </div>\n\t\t<div class=\"mbr-row mbr-jc-c price-container\" mbr-class=\"{'mbr-px-2': fullWidth, 'cardsAmount2': cardsAmount == 2}\">\n\t\t\t<div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-lg-3 md-pb': cardsAmount == 4,\n\t\t\t\t\t\t\t 'mbr-col-md-4': cardsAmount == 3,\n                             'mbr-col-md-6': cardsAmount < 3 || cardsAmount == 4,\n                             'mbr-col-xl-6': cardsAmount == 2,\n\t\t\t\t\t\t\t 'align-center': cardAlign == 2,\n\t\t\t\t\t\t\t 'align-right': cardAlign == 3,\n                         \t 'last-child': cardsAmount==1,\n                             'favoritePrice': favoriteItem == 1 &amp;&amp; showFavorite}\">\n\t\t\t\t<div class=\"card-wrapper\" mbr-class=\"{'mbr-pb-4': !transparentBg || showBorder}\">\n\t\t\t\t\t<div class=\"mbr-overlay\" mbr-if=\"!transparentBg\"></div>\n\t\t\t\t\t<div class=\"card-head\">\n\t\t\t\t\t\t<h3 class=\"card-title mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".card-head .card-title\">Capital One®<br>Venture Rewards Credit Card</h3>\n                        <h4 class=\"mbr-text mbr-fonts-style mbr-pt-2 mbr-regular\" mbr-theme-style=\"display-4\" data-app-selector=\".card-head .mbr-text\" mbr-if=\"showPriceSubtitle\">Earn 20,000 Membership<br>Rewards® Points</h4>\n                        <div class=\"icon-wrap mbr-pt-3\" mbr-if=\"showIcons\">\n                            <span mbr-icon class=\"amp-iconfont fa-credit-card fa\"></span>\n                        </div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"items-list-wrap\">\n\t\t\t\t\t\t<ul mbr-list mbr-list-grow=\"card1Items\" class=\"items-list mbr-px-3\">\n\t\t\t\t\t\t\t<li class=\"list-item\">\n\t\t\t\t\t\t\t\t<span mbr-text class=\"item-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" data-app-selector=\".items-list\">Earning unlimited 2%</span>\n\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t</ul>\n\t\t\t\t\t</div>\n                    <div class=\"card-price-wrap mbr-flex mbr-pt-4\" mbr-class=\"{'mbr-jc-c':cardAlign == 2, 'mbr-jc-e': cardAlign == 3}\">\n                        <span mbr-text class=\"price-value mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-2\" data-app-selector=\".price-value\">50</span>\n                        <div class=\"card-price mbr-flex mbr-column\">\n                            <span mbr-text class=\"price-sign mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-6\" data-app-selector=\".price-sign\">$</span>\n                            <span mbr-text class=\"price-caption mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".price-caption\" mbr-if=\"showPriceText\">per month</span>\n                        </div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-button mbr-section-btn mbr-pt-1\" mbr-buttons mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove, -mbrBtnAdd\"><a class=\"btn btn-md btn-secondary\" data-app-placeholder=\"Type Text\">VIEW MORE</a></div>\n\t\t\t\t</div>\n\t\t\t</div>\n\n\t\t\t<div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-lg-3 md-pb': cardsAmount == 4,\n\t\t\t\t\t\t\t 'mbr-col-md-4': cardsAmount == 3,\n                             'mbr-col-md-6': cardsAmount < 3 || cardsAmount == 4,\n                             'mbr-col-xl-6': cardsAmount == 2,\n\t\t\t\t\t\t\t 'align-center': cardAlign == 2,\n\t\t\t\t\t\t\t 'align-right': cardAlign == 3,\n                         \t 'last-child': cardsAmount==2,\n                             'favoritePrice': favoriteItem == 2 &amp;&amp; showFavorite}\" mbr-if=\"cardsAmount > 1\">\n\t\t\t\t<div class=\"card-wrapper\" mbr-class=\"{'mbr-pb-4': !transparentBg || showBorder}\">\n\t\t\t\t\t<div class=\"mbr-overlay\" mbr-if=\"!transparentBg\"></div>\n\t\t\t\t\t<div class=\"card-head\">\n\t\t\t\t\t\t<h3 class=\"card-title mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" data-app-