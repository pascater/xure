          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".text-content when (@showImgTitle), (@showImgText)": {
              "overflow": "hidden"
            },
            ".second-element-box, .elements-block, .carousel-img, .dots-wrapper": {
              "width": "100%"
            },
            ".elements-block, .second-element, .image-wrap, .slider-box": {
              "position": "relative"
            },
            ".second-element when (@showPadding)": {
              "@media (min-width: 992px)": {
                "& when not (@reverseContent)": {
                  "padding-right": "70px"
                },
                "& when (@reverseContent)": {
                  "padding-left": "70px"
                }
              },
              "@media (min-width: 1200px)": {
                "& when (@fullWidth)": {
                  "padding-left": "100px",
                  "padding-right": "100px"
                }
              }
            },
            ".elements-block when not (@transparentBg)": {
              ".mbr-overlay": {
                "background": "@cardBg",
                "opacity": "@cardOpacity"
              },
              ".image-wrap": {
                "margin": "0",
                "padding": "0"
              }
            },
            ".marker-wrap:before, .mbr-content-subtitle:before": {
              "display": "inline-block"
            },
            ".mbr-list when (@stylizedCounters = \"1\") and (@showCardList)": {
              "counter-reset": "decimal",
              ".marker-wrap:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg}rem\"",
                "& when (@countersBorder)": {
                  "border-bottom": "2px solid @primaryColor"
                }
              }
            },
            ".marker-wrap:before when (@stylizedCounters = \"2\") and (@showCardList)": {
              "content": "\"\"",
              "border": "3px solid @primaryColor",
              "width": "(@widthImg - 0.375rem)",
              "height": "(@widthImg - 0.375rem)",
              "box-sizing": "initial"
            },
            ".mbr-list .items-list when (@showCardList)": {
              "flex-direction": "row",
              "& when not (@stylizedCounters = \"3\")": {
                "align-items": "baseline"
              },
              ".marker-wrap, .icon-wrap": {
                "margin-right": "15px"
              },
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              },
              "& when ((@widthImg * 16) > 30)": {
                ".marker-wrap:before, .icon-wrap .amp-iconfont": {
                  "@media (max-width: 767px)": {
                    "font-size": "30px",
                    "width": "30px",
                    "height": "30px"
                  }
                }
              }
            },
            ".icon-wrap .amp-iconfont when (@stylizedCounters = \"3\") and (@showCardList)": {
              "color": "@primaryColor",
              "font-size": "~\"@{widthImg}rem\"",
              "width": "~\"@{widthImg}rem\""
            },
            "amp-carousel": {
              "height": "~\"@{slideHeight}px\" !important"
            },
            ".carousel-img": {
              "height": "(@slideHeight - 85px)",
              "margin": "0",
              "margin-bottom": "85px",
              ".gallery-img-wrap": {
                "& when not (@showImgTitle) and not (@showImgText)": {
                  "height": "(@slideHeight - 85px)"
                },
                "& when (@showImgTitle), (@showImgText)": {
                  "height": "(@slideHeight - 185px)"
                },
                "amp-img,img": {
                  "object-fit": "cover"
                }
              },
              ".text-wrap when (@showImgTitle), (@showImgText)": {
                "height": "100px",
                "padding-top": "35px",
                ".mbr-content-subtitle:before when (@lineText) and (@showImgText)": {
                  "content": "''",
                  "width": "25px",
                  "height": "3px",
                  "margin-right": "10px",
                  "vertical-align": "middle",
                  "background-color": "@primaryColor"
                }
              }
            },
            ".amp-carousel-button": {
              "background-color": "@buttonsColor",
              "top": "~\"calc(100% - 27px)\"",
              "opacity": "0.8",
              "&:after": {
                "border-color": "contrast(@buttonsColor, darken(@buttonsColor, 50%), #ffffff, 60%) !important"
              },
              "&:hover": {
                "opacity": "1"
              },
              "&.amp-carousel-button-prev": {
                "left": "0"
              },
              "&.amp-carousel-button-next": {
                "left": "55px"
              },
              "& when not (@countImg = 1), (@transparentBg)": {
                "@media (min-width: 992px)": {
                  "&.amp-carousel-button-prev": {
                    "left": "15px"
                  },
                  "&.amp-carousel-button-next": {
                    "left": "(15px + 55px)"
                  }
                }
              },
              "& when (@countImg = 2)": {
                "@media (min-width: 1200px)": {
                  "&.amp-carousel-button-prev": {
                    "left": "30px"
                  },
                  "&.amp-carousel-button-next": {
                    "left": "(30px + 55px)"
                  }
                }
              }
            },
            "@media (max-width: 991px)": {
              ".second-element": {
                "padding-bottom": "50px"
              }
            },
            ".gallery-img-wrap:after when not (@showImgOverlay)": {
              "width": "0"
            },
            ".gallery-img-wrap when (@showImgOverlay)": {
              "&:hover:after": {
                "opacity": "@imgOverlayOpacity",
                "background": "@imgOverlayColor"
              },
              "amp-img > a:before when (@showImgButton)": {
                "content": "\"＋\"",
                "position": "absolute",
                "top": "50%",
                "left": "50%",
                "font-size": "40px",
                "transform": "translate(-50%, -50%)",
                "z-index": "1",
                "color": "transparent",
                "transition": "color 0.2s"
              },
              "&:hover amp-img > a:before when (@showImgButton)": {
                "color": "contrast(@imgOverlayColor, darken(@imgOverlayColor, 50%), #ffffff, 60%)"
              }
            },
            ".element-text": {
              "color": "#84848c"
            }
          },
          "_name": "slider3",
          "_customHTML": "<section class=\"slider3 slider\" group=\"Teams\" plugins=\"amp-main, amp-carousel\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Width Images\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons\">    \n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons\">\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>    \n    <header>Images</header>\n        <input type=\"range\" inline=\"\" title=\"Slider Height\" name=\"slideHeight\" min=\"300\" max=\"700\" step=\"10\" value=\"410\">\n        <input type=\"range\" inline=\"\" title=\"Count Images Slider\" name=\"countImg\" min=\"1\" max=\"4\" step=\"1\" value=\"2\">\n        <select title=\"Sliders Count\" name=\"slidersAmount\">\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected=\"\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=