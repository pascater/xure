        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".second-element-box, .image-wrap, .tabContent": {
              "width": "100%"
            },
            ".second-element, .second-element-box, .image-wrap": {
              "position": "relative"
            },
            ".tabContent .second-element when (@showPadding)": {
              "@media (min-width: 992px)": {
                "& when not (@reverseContent)": {
                  "padding-left": "40px"
                },
                "& when (@reverseContent)": {
                  "padding-right": "40px"
                }
              },
              "@media (min-width: 1200px)": {
                "& when (@fullWidth)": {
                  "padding-left": "100px",
                  "padding-right": "100px"
                }
              }
            },
            ".tabContent when not (@transparentBg)": {
              ".mbr-overlay": {
                "background": "@cardBg",
                "opacity": "@cardOpacity"
              },
              ".image-wrap": {
                "padding": "0"
              }
            },
            "amp-img,img": {
              "object-fit": "cover",
              "@media (min-width: 768px)": {
                "height": "~\"@{heightImg}rem\""
              }
            },
            ".milestone:before when (@showBorderTop) and (@showMilestone)": {
              "content": "\"\"",
              "width": "100%",
              "height": "1px",
              "background-color": "@borderColor",
              "margin": "50px 0"
            },
            ".list1 .counters-wrap:before, .list1 .marker-wrap:before, .list2 .line-wrap:before, .list2 .marker-wrap:before": {
              "display": "inline-block"
            },
            ".list1 when (@showCardList1)": {
              "counter-reset": "decimal",
              ".counters-wrap:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg1Counters}rem\"",
                "line-height": "1.7",
                "& when (@countersBorder)": {
                  "border-bottom": "2px solid @primaryColor",
                  "line-height": "1.4"
                },
                "& when ((@widthImg1Counters * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "font-size": "30px"
                  }
                }
              },
              ".marker-wrap:before": {
                "content": "\"\"",
                "border": "3px solid @primaryColor",
                "width": "(@widthImg1Markers - 0.375rem)",
                "height": "(@widthImg1Markers - 0.375rem)",
                "box-sizing": "initial",
                "& when ((@widthImg1Markers * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "height": "30px",
                    "width": "30px"
                  }
                }
              },
              ".icon-wrap .amp-iconfont": {
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg1Icons}rem\"",
                "width": "~\"@{widthImg1Icons}rem\"",
                "& when ((@widthImg1Icons * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "font-size": "30px",
                    "width": "30px"
                  }
                }
              },
              ".marker-wrap, .icon-wrap, .counters-wrap": {
                "margin-right": "15px"
              },
              ".items-list:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".items-list": {
              "flex-direction": "row"
            },
            "@media (max-width: 767px)": {
              ".second-element when (@transparentBg)": {
                "padding-top": "50px"
              },
              ".milestone when (@showMilestone)": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                },
                "& > div:not(:first-child)": {
                  "margin-top": "20px"
                }
              }
            },
            ".list2 .line-wrap:before, .list1 .marker-wrap:before": {
              "margin-top": "9px"
            },
            ".list2 when (@showCardList2)": {
              ".line-wrap:before": {
                "content": "\"\"",
                "background-color": "@primaryColor",
                "margin-right": "5px",
                "width": "~\"@{widthImg2}rem\"",
                "height": "3px",
                "& when ((@widthImg2 * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "width": "30px"
                  }
                }
              },
              ".marker-wrap:before": {
                "content": "\"✓\"",
                "font-size": "~\"@{widthImg2}rem\"",
                "margin": "6px 13px 0 0 ",
                "& when ((@widthImg2 * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "font-size": "30px"
                  }
                }
              }
            },
            "@media (min-width: 768px)": {
              ".ampTabContainer": {
                "display": "flex",
                "flex-wrap": "wrap",
                "padding": "0"
              },
              ".milestone > .mbr-col-md-6 when (@showMilestone)": {
                "&:nth-child(2) when (@columnWidth = 5)": {
                  "margin-top": "20px"
                },
                "&:nth-child(3), &:nth-child(4)": {
                  "margin-top": "20px"
                },
                "&.mbr-col-xl-4:nth-child(3)": {
                  "@media (min-width: 1200px)": {
                    "margin-top": "0"
                  }
                },
                "& when (@showBorderRight)": {
                  "&:nth-child(2n)": {
                    "border-left": "1px solid @borderColor"
                  },
                  "&.mbr-col-xl-4:nth-child(3)": {
                    "@media (min-width: 1200px)": {
                      "border-left": "1px solid @borderColor"
                    }
                  }
                }
              }
            },
            ".tabContent": {
              "display": "none",
              "order": "1",
              "> div": {
                "padding": "55px 30px 30px",
                "@media (max-width: 991px)": {
                  "padding": "55px 15px 30px"
                },
                "order": "1",
                "background-color": "@SliderBg"
              }
            },
            ".tabButton": {
              "padding": "15px 30px",
              "@media (max-width: 991px)": {
                "padding": "10px 15px"
              },
              "@media (max-width: 767px)": {
                "padding": "14px 20px",
                "border": "1px solid darken(@SliderBg, 5.8%)",
                "&:not(.last-child)": {
                  "border-bottom": "none"
                }
              }
            },
            ".tabButton[selected]": {
              "color": "@primaryColor",
              "background-color": "@SliderBg",
              "outline": "none",
              "@media (max-width: 767px)": {
                "background-color": "transparent",
                "&:not(.last-child)": {
                  "border-bottom": "1px solid darken(@SliderBg, 5.8%)"
                }
              }
            },
            ".tabButton[selected] + .tabContent": {
              "display": "block"
            },
            ".element-text": {
              "color": "#84848c"
            }
          },
          "_name": "tab2",
          "_customHTML": "<section class=\"tab2\" group=\"Tabs &amp; Toggles\" plugins=\"amp-main, amp-selector\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone\">\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>    \n    <header>Text</header>\n        <input type=\"range\" inline=\"\" title=\"Width Image\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons\">\n        <input type=\"range\" inline=\"\" title=\"Height Image\" name=\"heightImg\" min=\"5\" max=\"60\" step=\"1\" value=\"25\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showCardSubtitle\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showCardText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showCardButtons\" checked=\"\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Padding\" name=\"showPadding\" condition=\"showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#bec1c7\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone)\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone)\">\n    <header>List1</header>   \n        <input type=\"checkbox\" title=\"List1\" name=\"showCardList1\">\n        <input type=\"range\" inline=\"\" title=\"Width Counters\" name=\"widthImg1Counters\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"0.8\" condition=\"showCardList1 &amp;&amp; ((stylizedCounters1Tab1 == 1 &amp;&amp; list1CountTab1>0) || (stylizedCounters1Tab2 == 1 &amp;&amp; list1CountTab2>0) || (stylizedCounters1Tab3 == 1 &amp;&amp; list1CountTab3>0 &amp;&amp; tabCount > 2) || (stylizedCounters1Tab4 == 1 &amp;&amp; list1CountTab4>0 &amp;&amp; tabCount > 3) || (stylizedCounters1Tab5 == 1 &amp;&amp; list1CountTab5>0 &amp;&amp; tabCount > 4) || (stylizedCounters1Tab6 == 1 &amp;&amp; list1CountTab6>0 &amp;&amp; tabCount > 5))\">\n        <input type=\"checkbox\" title=\"Border Bottom Counters\" name=\"countersBorder\" condition=\"showCardList1 &amp;&amp; ((stylizedCounters1Tab1 == 1 &amp;&amp; list1CountTab1>0) || (stylizedCounters1Tab2 == 1 &amp;&amp; list1CountTab2>0) || (stylizedCounters1Tab3 == 1 &amp;&amp; list1CountTab3>0 &amp;&amp; tabCount > 2) || (stylizedCounters1Tab4 == 1 &amp;&amp; list1CountTab4>0 &amp;&amp; tabCount > 3) || (stylizedCounters1Tab5 == 1 &amp;&amp; list1CountTab5>0 &amp;&amp; tabCount > 4) || (stylizedCounters1Tab6 == 1 &amp;&amp; list1CountTab6>0 &amp;&amp; tabCount > 5))\" checked=\"checked\">\n        <input type=\"range\" inline=\"\" title=\"Width Markers\" name=\"widthImg1Markers\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"0.8\" condition=\"showCardList1 &amp;&amp; ((stylizedCounters1Tab1 == 2 &amp;&amp; list1CountTab1>0) || (stylizedCounters1Tab2 == 2 &amp;&amp; list1CountTab2>0) || (stylizedCounters1Tab3 == 2 &amp;&amp; list1CountTab3>0 &amp;&amp; tabCount > 2) || (stylizedCounters1Tab4 == 2 &amp;&amp; list1CountTab4>0 &amp;&amp; tabCount > 3) || (stylizedCounters1Tab5 == 2 &amp;&amp; list1CountTab5>0 &amp;&amp; tabCount > 4) || (stylizedCounters1Tab6 == 2 &amp;&amp; list1CountTab6>0 &amp;&amp; tabCount > 5))\">\n        <input type=\"range\" inline=\"\" title=\"Width Icons\" name=\"widthImg1Icons\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"2.5\" condition=\"showCardList1 &amp;&amp; ((stylizedCounters1Tab1 == 3 &amp;&amp; list1CountTab1>0) || (stylizedCounters1Tab2 == 3 &amp;&amp; list1CountTab2>0) || (stylizedCounters1Tab3 == 3 &amp;&amp; list1CountTab3>0 &amp;&amp; tabCount > 2) || (stylizedCounters1Tab4 == 3 &amp;&amp; list1CountTab4>0 &amp;&amp; tabCount > 3) || (stylizedCounters1Tab5 == 3 &amp;&amp; list1CountTab5>0 &amp;&amp; tabCount > 4) || (stylizedCounters1Tab6 == 3 &amp;&amp; list1CountTab6>0 &amp;&amp; tabCount > 5))\">\n        <!--Tab1-->\n            <input type=\"range\" inline=\"\" title=\"Count Tab1\" name=\"list1Count